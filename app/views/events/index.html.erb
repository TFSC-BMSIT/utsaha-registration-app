<p id="notice"><%= notice %></p>

<h1>Event List</h1>

<table class="statistics table">
  <thead>
    <tr>
      <th>Event Name</th>
      <th>No. of Registrations</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.event_registrations.count %></td>
        <td><%= link_to 'Show', event %></td>
      </tr>
    <% end %>
  </tbody>
</table>
