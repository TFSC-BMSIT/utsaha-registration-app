<p id="notice"><%= notice %></p>

<h1>Listing Event Registrations</h1>

<table class="statistics table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Mobile no</th>
      <th>College</th>
      <th>Token</th>
      <th>Amount</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_registrations.each do |event_registration| %>
      <tr>
        <td><%= event_registration.name %></td>
        <td><%= event_registration.mobile_no %></td>
        <td><%= event_registration.college %></td>
        <td><%= event_registration.token %></td>
        <td><%= event_registration.amount %></td>
        <td><%= event_registration.user.name %></td>
        <td><%= link_to 'Show', event_registration %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event registration', new_event_registration_path %>
