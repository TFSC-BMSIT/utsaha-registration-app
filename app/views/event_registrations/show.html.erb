<main>
  <div class="container">
    <div class="col-lg-offset-3 col-lg-6 col-md-offset-2 col-md-8">

      <h1 class="text-center">Event Registration</h1>

      <table class="table table-bordered table-condensed">
        <tr>
          <td><strong>Name</strong></td>
          <td><%= @event_registration.name %></td>
        </tr>

        <tr>
          <td><strong>Mobile no</strong></td>
          <td><%= @event_registration.mobile_no %></td>
        </tr>

        <tr>
          <td><strong>College</strong></td>
          <td><%= @event_registration.college %></td>
        </tr>

        <tr>
          <td><strong>Token</strong></td>
          <td><strong><%= @event_registration.token %></strong></td>
        </tr>

        <tr>
          <td><strong>Amount</strong></td>
          <td><strong><%= @event_registration.amount %></strong></td>
        </tr>

        <tr>
          <td><strong>User</strong></td>
          <td><%= @event_registration.user.name %></td>
        </tr>
      </table>
      <%= link_to 'Back', event_registrations_path %>
    </div>
  </div>
</main>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>
